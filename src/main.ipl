
def add(a, b)
    return a + b

def subtract(a, b)
    return a - b

def multiply(a, b)
    return a * b

def divide(a, b)
    if b == 0
        return a
    else
        return a / b

def power(base, exponent)
    result = 1
    i = 0
    while i < exponent
        result = multiply(result, base)
        i = add(i, 1)
    return result

